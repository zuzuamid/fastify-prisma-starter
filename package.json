{
  "name": "nodejs-express-prisma-starter",
  "version": "1.0.0",
  "description": "A comprehensive Node.js backend starter template with Express.js, PostgreSQL, Prisma ORM, JWT authentication, file upload, and more",
  "main": "dist/server.js",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "nodejs",
    "express",
    "typescript",
    "postgresql",
    "prisma",
    "jwt",
    "authentication",
    "file-upload",
    "api",
    "backend",
    "starter",
    "template",
    "boilerplate"
  ],
  "author": {
    "name": "Ibn Abdullah",
    "email": "arfathossen541@gmail.com",
    "url": "https://github.com/ibnabdullah1"
  },
  "license": "MIT",
  "homepage": "https://github.com/ibnabdullah1/nodejs-express-prisma-starter#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ibnabdullah1/nodejs-express-prisma-starter.git"
  },
  "bugs": {
    "url": "https://github.com/ibnabdullah1/nodejs-express-prisma-starter/issues"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "start": "node dist/server.js",
    "build": "tsc && npm run copy:assets",
    "copy:assets": "cp -r src/templates dist/ && cp -r src/static dist/ 2>/dev/null || true",
    "build:clean": "rimraf dist && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "type-check": "tsc --noEmit",
    "create-module": "npx ts-node src/scripts/createModule.ts",
    "migrate": "npx prisma migrate dev",
    "migrate:deploy": "npx prisma migrate deploy",
    "migrate:reset": "npx prisma migrate reset",
    "migrate:status": "npx prisma migrate status",
    "db:seed": "npx ts-node src/scripts/seed.ts",
    "db:studio": "npx prisma studio",
    "db:generate": "npx prisma generate",
    "db:validate": "npx prisma validate",
    "db:push": "npx prisma db push",
    "db:pull": "npx prisma db pull",
    "postinstall": "npm run db:generate",
    "prebuild": "npm run type-check && npm run lint",
    "prestart": "npm run build",
    "docker:build": "docker build -t nodejs-express-prisma-starter .",
    "docker:run": "docker run -p 5000:5000 nodejs-express-prisma-starter",
    "docker:compose": "docker-compose up -d",
    "docker:compose:build": "docker-compose up --build -d",
    "docker:compose:down": "docker-compose down",
    "health": "curl -f http://localhost:5000/health || exit 1",
    "prepare": "husky install"
  },
  "dependencies": {
    "@prisma/client": "^5.11.0",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "cloudinary": "^2.0.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "http-status": "^1.7.4",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.4.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.12",
    "sslcommerz-lts": "^1.1.0",
    "stripe": "^17.4.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/luxon": "^3.4.2",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.26",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.14",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "prisma": "^5.11.0",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.2"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": ["<rootDir>/src"],
    "testMatch": ["**/__tests__/**/*.ts", "**/?(*.)+(spec|test).ts"],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/server.ts",
      "!src/scripts/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": ["text", "lcov", "html"]
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "@typescript-eslint/recommended",
      "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "rules": {
      "@typescript-eslint/no-unused-vars": "error",
      "@typescript-eslint/no-explicit-any": "warn",
      "prefer-const": "error",
      "no-var": "error"
    },
    "env": {
      "node": true,
      "es2021": true
    }
  },
  "nodemonConfig": {
    "watch": ["src"],
    "ext": "ts,js,json",
    "ignore": ["src/**/*.spec.ts", "src/**/*.test.ts"],
    "exec": "ts-node src/server.ts"
  }
}

